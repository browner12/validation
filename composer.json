{
    "name": "browner12/validation",
    "description": "validation package for use in Laravel projects",
    "keywords": [
        "browner12",
        "validation"
    ],
    "homepage": "https://github.com/browner12/validation",
    "license": "MIT",
    "authors": [
        {
            "name": "Andrew Brown",
            "email": "browner12@gmail.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": ">=5.6.0",
        "illuminate/contracts": "~5.2"
    },
    "require-dev": {
        "phpunit/phpunit": "5.*"
    },
    "autoload": {
        "psr-4": {
            "browner12\\validation\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "browner12\\validation\\Test\\": "tests"
        }
    },
    "scripts": {
        "test": "phpunit"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev"
        }
    }
}
